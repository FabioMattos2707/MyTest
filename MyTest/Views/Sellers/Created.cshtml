@model MyTest.Models.Seller
@{ 
    ViewData["Title"] = "Created";
}

<h2>
    @ViewData["Title"]
</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Created">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="BirthDate" class="control-label"></label>
                <input asp-for="BirthDate" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="BaseSalary" class="control-label"></label>
                <input asp-for="BaseSalary" class="form-control" />
            </div>
            <div>
                <input type="submit" value="Created" class="btn btn-info">
            </div>
        </form>
    </div>
</div>